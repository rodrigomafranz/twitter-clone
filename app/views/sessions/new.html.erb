<h1>Login</h1>

<%= form_for :session do |form| %>
	<div>
		<%= form.label :email %>
		<div>
			<%= form.email_field :email %>
		</div>
	</div>

	<div>
		<%= form.label :password_digest %>
		<div>
			<%= form.password_field :password_digest %>
		</div>
	</div>

	<div>
		<%= form.submit %>
	</div>

<% end %>